
static void	myascii(void)
{
	struct winsize	w;

	ioctl(STDOUT_FILENO, TIOCGWINSZ, &w);
	if (w.ws_col <= 100)
		return ;
	printf("\033[1;33m\n ███▄ ▄███▓ ██▓ ███▄    █  ██▓  ██████  ██░ ██ \
	▓█████  ██▓     ██▓    \033[0m\n");
	printf("\033[38;5;220m▓██▒▀█▀ ██▒▓██▒ ██ ▀█   █ ▓██▒▒██    ▒ ▓██░ ██▒\
	▓█   ▀ ▓██▒    ▓██▒    \033[0m\n");
	printf("\033[38;5;214m▓██    ▓██░▒██▒▓██  ▀█ ██▒▒██▒░ ▓██▄   ▒██▀▀██░\
	▒███   ▒██░    ▒██░    \033[0m\n");
	printf("\033[38;5;208m▒██    ▒██ ░██░▓██▒  ▐▌██▒░██░  ▒   ██▒░▓█ ░██ \
	▒▓█  ▄ ▒██░    ▒██░    \033[0m\n");
	printf("\033[38;5;202m▒██▒   ░██▒░██░▒██░   ▓██░░██░▒██████▒▒░▓█▒░██▓\
	░▒████▒░██████▒░██████▒\033[0m\n");
	printf("\033[38;5;166m░ ▒░   ░  ░░▓  ░ ▒░   ▒ ▒ ░▓  ▒ ▒▓▒ ▒ ░ ▒ ░░▒░▒\
	░░ ▒░ ░░ ▒░▓  ░░ ▒░▓  ░\033[0m\n");
	printf("\033[38;5;130m░  ░      ░ ▒ ░░ ░░   ░ ▒░ ▒ ░░ ░▒  ░ ░ ▒ ░▒░ ░ \
	░ ░  ░░ ░ ▒  ░░ ░ ▒  ░\033[0m\n");
	printf("\033[38;5;94m░      ░    ▒ ░   ░   ░ ░  ▒ ░░  ░  ░   ░  ░░ ░  \
	░     ░ ░     ░ ░   \033[0m\n");
	printf("\033[38;5;58m       ░    ░           ░  ░        ░   ░  ░  ░  \
	░  ░    ░  ░    ░  ░\033[0m\n");
}
